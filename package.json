{
  "name": "dist",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass --watch sass -o css",
    "compile:sass": "node-sass sass/test.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/test.css css/test.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css  -o css/test.prefix.css",
    "compress:css": "node-sass css/test.prefix.css css/test.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "dounia lasfar",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "concat": "^1.0.3",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
